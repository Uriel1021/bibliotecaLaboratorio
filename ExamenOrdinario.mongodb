//Crear base de datos
use bilotecaLaboratori
//Crear coleccion "usuarios" con reglas de inserción
db.createCollection("usuarios", {
   validator: {
        $jsonSchema: {
            bsonType: "object",
            required: ["matricula", "nombre", "apePaterno","apeMaterno", "correo"],
            properties: {
                matricula:{
                    bsonType:"string",
                    description:"Debe ser de tipo string"
                },
                nombre: {
                    bsonType: "string",
                    description: "Debe ser de tipo string"
                },
                apePaterno: {
                    bsonType: "string",
                    description: "Debe ser de tipo string"
                },
                apeMaterno: {
                    bsonType: "string",
                    description: "Debe ser de tipo string"
                },
                correo: {
                    bsonType : "string",
                    pattern : "@gmail\\.com$",
                    description: "debe de ser de tipo string + '@gmail.com'"
                },
                usuario:{
                    bsonType:"string",
                    description: "El usuario debe ser de tipo string"
                },
                contrasenia:{
                    bsonType:"string",
                    description: "La contraseña debe ser de tipo string"
                },
                telefono:{
                    bsonType:"string",
                    description:"El telefono debe ser de tipo string"
                },
                grupo: {
                    bsonType: "object",
                    required:["nombreGrupo","semestre","carrera"],
                    properties:{
                        nombreGrupo:{bsonType:"string"},
                        semestre: {
                            bsonType: "object",
                            required:["nombreSemestre"],
                            properties:{
                                nombreSemestre:{bsonType:"string"}
                            },
                            description: "Debe ser de tipo string"
                        },
                        carrera: {
                            bsonType: "object",
                            required:["nombreCarrera"],
                            properties:{
                                nombreGrupo:{bsonType:"string"}
                            },
                            description: "Debe ser de tipo string"
                        },
                    },
                    description: "El nombreGrupo debe ser de tipo string. Carrera y semestre deben ser de tipos object"
                },
                tipoUsuario: {
                    bsonType: "object",
                    required:["nombreUsuario"],
                    properties:{
                        nombreUsuario:{bsonType:"string"}
                    },
                    description: "El tipo usuario debe ser de tipo object"
                },
                puesto: {
                    bsonType: "object",
                    required:["nombrePuesto","areaPuesto"],
                    properties:{
                        nombrePuesto:{bsonType:"string"},
                        areaPuesto:{
                            bsonType:"object",
                            required:["area"],
                            properties:{
                                area:{bsonType:"string"}
                            },
                            description: "El area debe de ser tipo string"
                        },
                    },
                    description: "el nombre del puesto debe de ser de tipo string y area puesto de tipo object"
                }
            }
        }
    }
})

