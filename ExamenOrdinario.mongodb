//Consulta en documentos añadidos

//01. Mostrar los usuarios que sean alumnos
db.usuarios.find({'tipoUsuario.nombreUsuario':'alumno'}).pretty().limit(5)

//02. Mostrar todos los que sean de la carrera de informatica
db.usuarios.find({'grupo.carrera.nombreCarrera':'Licenciatura en informatica'});

//03. Mostrar todos los alumnos que sean de la carrera de biologia
db.usuarios.find({'tipoUsuario.nombreUsuario':'alumno','grupo.carrera.nombreCarrera':'Licenciatura en biologia'},{nombre:1,apePaterno:1,apeMaterno:1}).pretty()

//04, Mostrar todas las carreras
db.usuarios.find({'grupo.carrera':{$exists:true}},{'grupo.carrera.nombreCarrera':1}).pretty().limit(5)

//05. Sumar el total del los alumnos 
db.usuarios.find({'tipoUsuario.nombreUsuario':'alumno'}).count()

//06. Mostrar todos los usuarios que su area sea en servicios escolares
db.usuarios.find({'puesto.areaPuesto.area':'Servicios Escolares'},{nombre:1,apePaterno:1,apeMaterno:1}).pretty().limit(7)

//07. Mostrar todos los usuarios que su puesto sea secretario y que cuenten con un telefono
db.usuarios.find({telefono:{$exists:true},'puesto.nombrePuesto':'Secretario'}).pretty()

//08. Mostrar todos los puesto que existe
db.usuarios.find({puesto:{$exists:true}},{'puesto.nombrePuesto':1}).pretty().limit(5)

//09. Actualizar un documento
db.usuario.updateOne({matricula:{$eq:'0119030072'}},{$set:{telefono:'9511234567'}})

//10. Actualizar un documento añadado
db.usuario.updateOne({matricula:{$eq:'0119030074'}},{$set:{'puesto.nombrePuesto':'Jefe de departamento de servicios escolares'}})

//11. Mostrar con Java Script los usuarios
db.usuarios.find().forEach(usuarios => print("Nombre Completo: " + usuarios.nombre + " " + usuarios.apePaterno + " " + usuarios.apeMaterno))

//12. Mostrar los usuarios que sean encargados
db.usuarios.find({'tipoUsuario.nombreUsuario':'Encargado'},{nombre:1,apePaterno:1,apeMaterno:1}).pretty()

//13. Mostar todas las categorias de laboratorio
db.recursos.find({materialLaboratorio:{$exists:true}},{'categoria.nombreCategoria':1}).pretty()

//14.Mostrar todas las categorias de biblioteca
db.recursos.find({materialBiblioteca:{$exists:true}},{'categoria.nombreCategoria':1}).pretty()

//15. Mostrar los materiales disponibles en los laboratorios
db.recursos.find({materialLaboratorio:{$exists:true},'disponibilidad.disponible':{$eq:1}},{'materialLaboratorio.nombre':1})

//16.Mostrar los libros disoponibles en la biblioteca
db.recursos.find({materialBiblioteca:{$exists:true},'disponibilidad.disponible':{$eq:1}},{'materialBiblioteca.titulo':1}).pretty()

//17. Mostrar los materiales de laboratorio que esten disponibles y su estado sea "excelente" y mostrarlo con JavaScript
db.recursos.find({materialLaboratorio:{$exists:true},'estadoFisico.estado':'Excelente','disponibilidad.disponible':1}).forEach(recursos => print("Nombre del material: " + recursos.materialLaboratorio.nombre))

//18.Mostrar
db.recursos.find({materialBiblioteca:{$exists:true},'estadoFisico.estado':'Malo','disponibilidad.disponible':1}).forEach(recursos => print("Nombre del libro: " + recursos.materialBiblioteca.titulo))

//19.
